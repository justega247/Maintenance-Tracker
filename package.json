    "url": "https://github.com/justega247/Maintenance-Tracker/issues"
  },
  "homepage": "https://github.com/justega247/Maintenance-Tracker#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "lodash.isempty": "^4.4.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "winston": "^3.0.0-rc5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
