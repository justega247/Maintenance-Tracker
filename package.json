    "test-watch": "nodemon --exec \"npm test\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrate": "babel-node server/models/database.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justega247/Maintenance-Tracker.git"
